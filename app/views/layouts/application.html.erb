<!DOCTYPE html>
<html>
<head>
  <title>Tutorgrapeapi</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%# stylesheet_link_tag  , "ss-standard", "ss-social", "fonts/museo/stylesheet", "fonts/ss-social/ss-social"%>
  <%= javascript_include_tag "application" %>

  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=7, IE=9, IE=10">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">

  <%= csrf_meta_tags %>
</head>
<body>



<%= render 'layouts/header' %>
<% if flash.notice %>
  <p class="profile-setup-notice"><%= flash.notice %></p>
<% end %>
<div class="app-body-obj">
<%= yield %>
</div>
  <!-- FOOTER OBJECT -->
  <div class="footer-obj">
    <div class="wrapper">
      <div class="grid">

        <div class="grid__cell">
          <h2>{{ FOOTER GOES HERE }}</h2>
        </div>

      </div>
    </div>
  </div>
  <!-- FOOTER OBJECT END -->

<script src="//code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
<script>
  window.jQuery || document.write('<script src="scripts/vendor/jquery-2.0.2.js"><\/script>')
</script>
<%= javascript_include_tag "scripts" %>
</body>
</html>
